{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "arsd",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "create": true,
        "hide": true
      },
      "clipboard": {
        "all": false,
        "readText": true,
        "writeText": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "io.rsb.arsd",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": "default-src 'self' localhost",
      "devCsp": "default-src 'self' localhost"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "arsd - AWS Role Speed Demon",
        "width": 800,
        "height": 1200
      }
    ],
    "cli": {
      "description": "arsd - AWS Role Speed Demon",
      "subcommands": {
        "cs": {
          "description": "Open a session in the system browser"
        }
      }

    }
  }
}
